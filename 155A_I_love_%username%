#include<iostream>
using namespace std;
int main()
{
    int a,count=0,max,min;
    cin>>a;
    int ar[a];
    for(int i=0; i<a; i++)
    {
        cin>>ar[i];
    }
    max=ar[0];
    min=ar[0];
    for(int i=0; i<a; i++)
    {
        if(ar[i]>max)
        {
            max=ar[i];
            count++;
        }
        if(ar[i]<min)
        {
            min=ar[i];
            count++;
        }
    }
    cout<<count;
}
